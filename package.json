    "type": "module",
    "devDependencies": {
        "@eslint/js": "9.39.1",
        "@webgpu/types": "^0.1.66",
        "@yutengjing/ts-fix": "^0.0.1",
        "eslint": "9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "globals": "^16.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.6.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.3",
        "vite": "^7.2.2"
