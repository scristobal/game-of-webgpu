{
    "name": "webgpu",
    "version": "1.0.0",
    "type": "module",
    "devDependencies": {
        "@eslint/js": "9.20.0",
        "@webgpu/types": "^0.1.54",
        "@yutengjing/ts-fix": "^0.0.1",
        "eslint": "9.20.1",
        "eslint-config-prettier": "^10.0.1",
        "globals": "^15.14.0",
        "hint": "^7.1.13",
        "npm-check-updates": "^17.1.14",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.5.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.24.0",
        "vite": "^6.1.0"
    },
    "scripts": {
        "build": "vite build",
        "check": "run-p check:*",
        "check:format": "prettier . --check",
        "check:lint": "eslint",
        "check:types": "tsc",
        "dev": "vite --open",
        "fix": "run-s fix:*",
        "fix:format": "prettier . --write",
        "fix:lint": "eslint --fix",
        "fix:types": "ts-fix",
        "preview": "vite preview --open",
        "update": "npm-check-updates -i"
    }
}
